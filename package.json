{
  "name": "zombienet-extension",
  "displayName": "zombienet-extension",
  "description": "Zombienet extension",
  "version": "0.0.2",
  "publisher": "paritytech",
  "icon": "resources/zombienet_256px.png",
  "authors": [
    {"name": "Wilfried Kopp aka Chevdor"},
    {"name": "Javier Viola"}
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/paritytech/zombienet-vscode-extension.git"
  },
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "zndsl",
        "path": "./snippets/snippets.code-snippets"
      }
    ],
    "languages": [
      {
        "id": "zndsl",
        "extensions": [
          ".feature",
          ".zndsl"
        ],
        "aliases": [
          "ZombieNet"
        ]
      }
    ],
    "files.associations": {
      "*.feature": "zndsl"
    }
  }
}
